{
  "name": "Price",
  "base": "persistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [
      "product"
    ]
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "amount": {
      "type": "number",
      "required": true
    },
    "customPortion": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "product": {
      "type": "belongsTo",
      "model": "Product",
      "foreignKey": ""
    },
    "discount": {
      "type": "belongsTo",
      "model": "Discount",
      "foreignKey": ""
    },
    "catPortion": {
      "type": "belongsTo",
      "model": "CatPortion",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}

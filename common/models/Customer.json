{
  "name": "Customer",
  "base": "persistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "scope": {
    "include": [
      "friendships"
    ]
  },
  "properties": {
    "notifications": {
      "type": "string"
    },
    "message": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": ""
    },
    "friendships": {
      "type": "hasMany",
      "model": "Friendship",
      "foreignKey": ""
    },
    "visits": {
      "type": "hasMany",
      "model": "Visit",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}

{
  "name": "Product",
  "base": "persistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "available": {
      "type": "boolean",
      "required": true,
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "catStatus": {
      "type": "belongsTo",
      "model": "CatStatus",
      "foreignKey": ""
    },
    "prices": {
      "type": "hasMany",
      "model": "Price",
      "foreignKey": ""
    },
    "place": {
      "type": "belongsTo",
      "model": "Place",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}

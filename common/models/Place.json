{
  "name": "Place",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "latitude": {
      "type": "geopoint"
    },
    "longitude": {
      "type": "geopoint"
    },
    "ranking": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "country": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "address": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "visits": {
      "type": "hasMany",
      "model": "Visit",
      "foreignKey": ""
    },
    "products": {
      "type": "hasMany",
      "model": "Product",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}

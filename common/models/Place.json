{
  "name": "Place",
  "base": "persistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "geopoint"
    },
    "ranking": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "country": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "zipCode": {
      "type": "number"
    },
    "phone": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "visits": {
      "type": "hasMany",
      "model": "Visit",
      "foreignKey": ""
    },
    "products": {
      "type": "hasMany",
      "model": "Product",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
